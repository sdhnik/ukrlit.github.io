data = '[{"text" : "У Павлуші та Яви була прекрасна, благородна ідея ", "answer1" : "провести біля корівника трамвайні колії;", "answer2" : "провести під птахофермою метро;", "answer3" : "провести під свинарником метро", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Ява - це ", "answer1" : "найкращий друзяка і напарник Павлуші;", "answer2" : "марка мотоцикла;", "answer3" : "острів в Індійському океані", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Лекція, під час якої хлопці випустили пугутькало, називалася", "answer1" : "«Виховання дітей на вулиці»;", "answer2" : "«Батьки і діти»;", "answer3" : "«Виховання дітей у сім’ї»", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Ява і Павлуша хотіли вперше в історії Васюківки влаштувати", "answer1" : "бій без правил;", "answer2" : "бій биків;", "answer3" : "бій півнів", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Другою кандидатурою на бій биків був ", "answer1" : "цап Жора;", "answer2" : "корова Контрибуція;", "answer3" : "бугай Петька", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Контрибуція підкинула Яву високо в небо ", "answer1" : "рогами;", "answer2" : "мордою;", "answer3" : "копитами", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Побачивши діда Вараву хлопці заклякли, присівши,-", "answer1" : "мов курчата перед шулікою;", "answer2" : "настороживши вуха;", "answer3" : "води в рот набравши", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Ява врятованому кудлатому, капловухому рудому цуценяті дав ім’я ", "answer1" : "Полкан;", "answer2" : "Жучка;", "answer3" : "Собакевич", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "До четвертого класу Павлуша з Явою не дружив, бо Рень був", "answer1" : "хвастливий;", "answer2" : "вредний;", "answer3" : "тихенький;", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Бомбили кавунами «військовий об’єкт» хлопці кілька хвилин, бо", "answer1" : "самим страшно стало і совісно;", "answer2" : "руки заболіли;", "answer3" : "лилася червона кавунова кров", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Від діда Салимона Павлушу врятував", "answer1" : "Стьопка Карафолька;", "answer2" : "Гришка Сало;", "answer3" : "Ява Рень", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "На першому засіданні гуртка вибрали п’єсу", "answer1" : "«Аудитор»;", "answer2" : "«Ревізор»;", "answer3" : "«Реалізатор»", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Вивчити слова п’єси було ", "answer1" : "гірше за будь-які уроки;", "answer2" : "гірше, ніж вивчити вірш напам’ять;", "answer3" : "гірше, ніж контрольна робота", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Що це «правильна п’єса» сказав ", "answer1" : "Ява Рень;", "answer2" : "дід Салимон;", "answer3" : "Стьопка Карафолька", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Переживання на сцені було таке,", "answer1" : "наче вперше виліз на вершечок верби;", "answer2" : "наче вперше стрибнув з верби у річку;", "answer3" : "наче вперше переплив річку", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Хлопці бігли навмання", "answer1" : "по безлюдному селу;", "answer2" : "по широкій дорозі;", "answer3" : "берегом річки", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Носа у книгу впхне і сидить під грушею нищечком", "answer1" : "Ява Рень;", "answer2" : "Стьопа Карафолька;", "answer3" : "Коля Кагарлицький", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Хворого Яву дратувала", "answer1" : "спритність сестри Яришки;", "answer2" : "надокучливість Яришки;", "answer3" : "усмішка сестри", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Хлопець подумав, що його рахунки з життям майже зведені, коли", "answer1" : "прийшла медсестра;", "answer2" : "кольнуло в боці;", "answer3" : "подарував сестрі велосипед", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Голова Яви крутилася від ", "answer1" : "того, що багато з’їв;", "answer2" : "від високої температури;", "answer3" : "довгого лежання", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Щастя так засліпило Яришку, що вона", "answer1" : "весело посміхалася брату;", "answer2" : "нічого не бачила;", "answer3" : "думала про брата;", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Ява навіть перед смертю не міг спокійно дивитися, як", "answer1" : "гине велосипед;", "answer2" : "лежить під колесом сестра;", "answer3" : "як друзі граються м’ячем", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Сонце засяяло з неба, заспівали пташки, коли хлопчик почув від сестри ", "answer1" : "любий братику;", "answer2" : "так тобі і треба;", "answer3" : "зараза чортова («загаза чортова»)", "answer4" : "", "correctAnswer" : "2", "img" : "n"}]';