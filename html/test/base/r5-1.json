data = '[{"text" : "У дитинстві майбутнього байкаря жартома називали", "answer1" : "«овочевим принцем»;", "answer2" : "«квітчастим королем»;", "answer3" : "«пахучим монархом»", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Перший вірш Л.Глібова російською мовою називався", "answer1" : "«Надія»;", "answer2" : "«Мрія»;", "answer3" : "«Сон»", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "За першу збірочку віршів юний поет отримав від гімназійного керівництва", "answer1" : "подяку;", "answer2" : "догану;", "answer3" : "нагороду", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Твори для дітей у львівському журналі «Дзвінок» письменник друкував під псевдонімом", "answer1" : "дідусь Кенар;", "answer2" : "дідусь Жайвір;", "answer3" : "дідусь Сич", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "«За стряпчого, як завсігди годиться, була приставлена »", "answer1" : "Лисиця;", "answer2" : "нікчемна Шкапа;", "answer3" : "старенький Цап", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Часто вночі Щука відправляла линів, карасів, щупачка", "answer1" : "Ослу;", "answer2" : "Цапу;", "answer3" : "Лисиці", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Судді підтримали думку Лисички і ", "answer1" : "Щуку на вербі повісити звеліли;", "answer2" : "Щуку вирішили отруїти;", "answer3" : "Щуку кинули – у річку", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "«Вітер тихесенько гойдає, мов панночку яку…»", "answer1" : "Муху-ледащицю;", "answer2" : "Бджолу-трудівницю;", "answer3" : "Осу", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "«Та ніколи мені сидіти», - сказала ", "answer1" : "Оса;", "answer2" : "Бджола-трудівниця;", "answer3" : "Муха-ледащиця", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "«Не можна в двері – я в кватирку або пролізу в іншу дірку», - сказала ", "answer1" : "Муха-ледащиця;", "answer2" : "Бджола-трудівниця;", "answer3" : "Оса", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Побачивши здорового Вола, Жаба вирішила", "answer1" : "погрітися на сонечку;", "answer2" : "подивитися, як він воду п’є;", "answer3" : "надутися, щоб стати більшою", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Жаба нікого не слухала і ", "answer1" : "добилася свого;", "answer2" : "з натуги луснула – та й одубіла;", "answer3" : "вирішила не зупинятися на досягнутому", "answer4" : "", "correctAnswer" : "1", "img" : "n"}]';