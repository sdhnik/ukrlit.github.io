data = '[{"text" : "Степан Руданський автор пісні", "answer1" : "«Молитва»;", "answer2" : "«Повій, вітре, на Вкраїну»;", "answer3" : "«Гей, видно село»", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Ім’я Степан у перекладі на українську мову означає", "answer1" : "букет;", "answer2" : "вінок;", "answer3" : "квітка", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Різновид гумору, введений у літературу Степаном Руданським, називається", "answer1" : "усмішка;", "answer2" : "співомовка;", "answer3" : "анекдот", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Короткий гумористичний вірш, побудований на народному анекдоті, приказці, афоризмі або якомусь казковому мотиві, - це", "answer1" : "співомовка;", "answer2" : "усмішка;", "answer3" : "анекдот", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Іван побратався і помандрував по світу з", "answer1" : "сусідом;", "answer2" : "паном;", "answer3" : "товаришем", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Пан задумав ", "answer1" : "хлопа ошукати;", "answer2" : "з хлопцем потоваришувати;", "answer3" : "у хлопа погостювати", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Якби в Івана було поле, то він ", "answer1" : "орав би, хлібом засівав би його;", "answer2" : "збудував би місто;", "answer3" : "поставив палац, крамниці", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Засмажена гуска дістанеться тому,", "answer1" : "хто краще буде працювати;", "answer2" : "хто раніше встане;", "answer3" : "кому присниться краща закуска", "answer4" : "", "correctAnswer" : "2", "img" : "n"}, {"text" : "Слова «Хтів когось ошукати, та й сам ошукався» сказав", "answer1" : "пан;", "answer2" : "Іван;", "answer3" : "автор", "answer4" : "", "correctAnswer" : "1", "img" : "n"}, {"text" : "Загадки короля відгадав і багатство отримав", "answer1" : "козак із Січі;", "answer2" : "багатий пан;", "answer3" : "Іван", "answer4" : "", "correctAnswer" : "0", "img" : "n"}, {"text" : "Задивившись на вуса запорожців, ляхи вирішили дати їм", "answer1" : "сметани;", "answer2" : "щільник меду;", "answer3" : "узвару", "answer4" : "", "correctAnswer" : "0", "img" : "n"}]';