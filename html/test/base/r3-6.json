data = '[{"text" : "Письменниця Емма Андієвська народилась у", "answer1" : "Німеччині;", "answer2" : "Україні;", "answer3" : "Канаді;", "answer4" : "США", "correctAnswer" : "1", "img" : "n"}, {"text" : "Чому Андієвська покинула межі України і виїхала за кордон?", "answer1" : "вийшла заміж за іноземця;", "answer2" : "там почала друкуватись, була змушена залишитись із-за роботи;", "answer3" : "її родина емігрувала ще під час війни;", "answer4" : "поїхала заробляти гроші в молоді роки і залишилась", "correctAnswer" : "2", "img" : "n"}, {"text" : "Якими видами мистецтва вправно володіє Емма Андієвська?", "answer1" : "танці, спів;", "answer2" : "живопис, скульптура;", "answer3" : "музика, література;", "answer4" : "живопис, література", "correctAnswer" : "3", "img" : "n"}, {"text" : "Твори яких жанрів літератури створює письменниця?", "answer1" : "лірика, епос;", "answer2" : "лірика, драма;", "answer3" : "драма, ліро-епос;", "answer4" : "ліро-епос, лірика", "correctAnswer" : "0", "img" : "n"}, {"text" : "Де народилась риба? («Говорюща риба»)", "answer1" : "у річці;", "answer2" : "у малій воді;", "answer3" : "в озері;", "answer4" : "у великих водах", "correctAnswer" : "3", "img" : "n"}, {"text" : "У якій сім’ї?", "answer1" : "у неї не було сім’ї", "answer2" : "у сім’ї акул;", "answer3" : "невідомо;", "answer4" : "у сім’ї поважних риб", "correctAnswer" : "3", "img" : "n"}, {"text" : "Яким чином потрапив у країну яян кволий дідусь?", "answer1" : "випав за борт корабля, на якому служив юнгою, під час сварки;", "answer2" : "впав із даху свого нового будинку ще в молоді роки;", "answer3" : "втратив свідомість під час землетрусу;", "answer4" : "він тут народився і виріс", "correctAnswer" : "0", "img" : "n"}, {"text" : "Чому запитання пастушка почув лише один чоловік?", "answer1" : "всі інші яяни були занадто заклопотані будівництвом власних веж;", "answer2" : "нікого із жителів міста не було на вулицях, бо був дуже спекотний день;", "answer3" : "кожен яянин знає тільки своє «я»